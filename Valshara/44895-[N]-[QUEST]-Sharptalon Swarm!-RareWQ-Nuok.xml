      <![CDATA[public bool OnQuest => Lua.GetReturnVal<bool>(@"SetMapByID(1018)
local taskInfo = C_TaskQuest.GetQuestsForPlayerByMapID(1018)
if (taskInfo) then
    for i, info  in ipairs(taskInfo) do
        if info.questId == 44895 then
            return true;
            end
    end
end
return false;", 0);]]>
