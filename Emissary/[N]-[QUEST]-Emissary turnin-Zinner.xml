		<CustomBehavior    File="UseItem" ItemId="141605" WaitTime="2000" />
		</If>
		
		<!-- Highmountain Tribes -->
		<If Condition="HasQuest(42233) &amp;&amp; IsQuestCompleted(42233)" >
			<MoveTo DestName="Thunder Totem" X="4156.51" Y="4374.45" Z="768.16" />
