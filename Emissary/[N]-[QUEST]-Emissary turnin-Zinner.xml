		</If>
	
		<!-- The wardens Emissary -->
		<If Condition="HasQuest(42422) &amp;&amp; IsQuestCompleted(42422)" >
			<MoveTo DestName="Marin Bladewing" X="-1441.083" Y="6656.882" Z="12.00175" />
				<TurnIn QuestName="The Wardens" QuestId="42422" TurnInName="Marin Bladewing" TurnInId="107379" X="-1441.083" Y="6656.882" Z="12.00175" />
		</If>
		
		
		<!-- Court of Farondis Emissary -->
